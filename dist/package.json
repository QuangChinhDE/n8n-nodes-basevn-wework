{
    "name": "n8n-nodes-basevn-wework",
    "version": "1.0.9",
    "description": "n8n node for BaseVN - App Wework API - Manage projects, tasks, departments with flexible field structures and webhook triggers",
    "license": "MIT",
    "homepage": "https://github.com/QuangChinhDE/n8n-nodes-basevn-wework",
    "keywords": [
        "n8n-community-node-package",
        "basevn",
        "wework",
        "project-management",
        "task-management",
        "api-integration"
    ],
    "author": {
        "name": "QuangChinh",
        "email": "quangchinh@basevn.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/QuangChinhDE/n8n-nodes-basevn-wework.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "npm run build && npm publish --access public",
        "prepublishOnly": "echo 'Use npm run release'"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/BaseWeworkApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/BaseWework/BaseWework.node.js",
            "dist/nodes/BaseWework/BaseWeworkTrigger.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
